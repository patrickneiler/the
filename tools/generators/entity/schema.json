{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "entity",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Entity Name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "projectName": {
      "type": "string",
      "description": "Project Name",
      "$default": {
        "$source": "projectName"
      }
    }
  },
  "required": ["name", "projectName"]
}